# QobuzProxy Docker Environment Variables
# Copy this file to .env and fill in your values

# Required: Qobuz account credentials
QOBUZ_EMAIL=your-email@example.com
QOBUZ_PASSWORD=your-password

# Optional: Audio quality
# 5 = MP3 320kbps
# 6 = FLAC CD Quality (16-bit/44.1kHz)
# 7 = FLAC Hi-Res (24-bit/96kHz)
# 27 = FLAC Hi-Res Max (24-bit/192kHz)
QOBUZ_MAX_QUALITY=27

# Optional: Device name shown in Qobuz app
QOBUZPROXY_DEVICE_NAME=QobuzProxy

# Backend type: "dlna" or "local"
# QOBUZPROXY_BACKEND=dlna

# DLNA backend settings (when using backend type "dlna")
QOBUZPROXY_DLNA_IP=192.168.1.50
QOBUZPROXY_DLNA_PORT=1400
# QOBUZPROXY_DLNA_FIXED_VOLUME=false
QOBUZPROXY_PROXY_PORT=7120

# Local audio backend settings (when using backend type "local")
# QOBUZPROXY_AUDIO_DEVICE=default
# QOBUZPROXY_AUDIO_BUFFER_SIZE=2048

# Optional: Server port
QOBUZPROXY_HTTP_PORT=8689

# Optional: Log level (debug, info, warning, error)
QOBUZPROXY_LOG_LEVEL=info
